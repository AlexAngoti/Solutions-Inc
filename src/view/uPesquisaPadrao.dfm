  object qryPadrao: TFDQuery
    Connection = dm.FDConnection
    SQL.Strings = (
      
        'SELECT p.id as "Id", p.nomerazao as "Nome", p.cnpjcpf as "CPF / ' +
        'CNPJ"'
      'FROM pessoa p '
      'WHERE p.tipopessoa = 0 '
      '  and p.situacao = 0 '
      'ORDER by p.nomerazao asc')
